{
  "extends": ["tslint:latest"],
  "rules": {
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "jsx-double"],
    "semicolon": [true, "never"],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ]
  }
}
