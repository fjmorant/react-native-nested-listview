{
  "name": "react-native-nested-listview",
  "version": "0.6.0",
  "description": "Nested Listview for React native",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest -u && codecov",
    "lint": "eslint --cache ./src",
    "tslint": "tslint -p .",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "tsc -p ./ --outDir dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjmorant/react-native-nested-listview.git"
  },
  "dependencies": {
    "@types/lodash.isequal": "4.5.5",
    "lodash.isequal": "4.5.0",
    "shortid": "2.2.14"
  },
  "peerDependencies": {
    "react": "16.2.0",
    "react-native": "~0.54.2"
  },
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/react": "16.8.23",
    "@types/react-native": "0.60.2",
    "@types/react-test-renderer": "16.8.2",
    "@types/shortid": "0.0.29",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-preset-react-native": "4.0.1",
    "eslint": "6.1.0",
    "eslint-plugin-flowtype": "3.12.1",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-native": "3.7.0",
    "flow-bin": "0.103.0",
    "istanbul": "0.4.5",
    "istanbul-api": "2.1.6",
    "istanbul-reports": "2.2.6",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "prettier-eslint": "9.0.0",
    "react-test-renderer": "16.8.6",
    "ts-jest": "24.0.2",
    "tslint": "5.18.0",
    "typescript": "3.5.3"
  },
  "keywords": ["react", "native", "list", "nested"],
  "author": "Javier Morant",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjmorant/react-native-nested-listview/issues"
  },
  "homepage": "https://github.com/fjmorant/react-native-nested-listview#readme",
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)|react-native|react-navigation|react-clone-referenced-element|mobx-react)"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "modulePathIgnorePatterns": ["<rootDir>/examples/"],
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/examples/",
      "<rootDir>/lib/",
      "<rootDir>/dist/"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    }
  }
}
