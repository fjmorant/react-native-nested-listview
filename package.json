{
  "name": "react-native-nested-listview",
  "version": "0.3.0",
  "description": "Nested Listview for React native",
  "scripts": {
    "test": "jest  && codecov",
    "lint": "eslint --cache .",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjmorant/react-native-nested-listview.git"
  },
  "dependencies": {
    "codecov": "^3.0.0",
    "react": "16.2.0",
    "react-native": "0.52.0",
    "shortid": "^2.2.8"
  },
  "peerDependencies": {
    "react": "16.2.0",
    "react-native": "0.52.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/react": "^16.0.40",
    "@types/react-native": "^0.52.17",
    "@types/react-test-renderer": "^16.0.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.4.1",
    "babel-preset-react-native": "^4.0.0",
    "eslint": "^4.9.0",
    "eslint-plugin-flowtype": "^2.36.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-react-native": "^3.1.0",
    "flow-bin": "^0.67.1",
    "jest": "^22.4.2",
    "prettier": "^1.7.0",
    "prettier-eslint": "^8.2.1",
    "react-test-renderer": "16.2.0",
    "ts-jest": "^22.4.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "keywords": ["react", "native", "list", "nested"],
  "author": "Javier Morant",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjmorant/react-native-nested-listview/issues"
  },
  "homepage": "https://github.com/fjmorant/react-native-nested-listview#readme",
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)|react-native|react-navigation|react-clone-referenced-element|mobx-react)"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "modulePathIgnorePatterns": ["<rootDir>/node_modules"],
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/examples/",
      "<rootDir>/lib/"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    }
  }
}
